{
  "name": "postcss-combine-duplicated-selectors",
  "version": "2.1.0",
  "description": "automatically keep css selectors unique",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "babel src -d dist --no-comments",
    "prepublish": "rimraf dist && npm run build",
    "pretest": "babel test/_test-factory.es -o test/_test-factory.js --no-comments",
    "test": "npm run build && ava && eslint --ext md,js,es . && remark *.md -u remark-lint -q --no-stdout"
  },
  "keywords": [
    "postcss-plugin",
    "selector"
  ],
  "author": {
    "name": "Christian Murphy",
    "email": "Christian.Murphy.42@gmail.com",
    "url": "https://github.com/ChristianMurphy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChristianMurphy/postcss-combine-duplicated-selectors.git"
  },
  "bugs": {
    "url": "https://github.com/ChristianMurphy/postcss-combine-duplicated-selectors/issues"
  },
  "license": "MIT",
  "dependencies": {
    "postcss": "^5.0.21",
    "postcss-selector-parser": "^2.1.0"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.10.1",
    "babel-plugin-transform-es2015-modules-commonjs-simple": "^6.7.4",
    "eslint": "^3.0.1",
    "eslint-config-google": "^0.6.0",
    "eslint-plugin-ava": "^2.5.0",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-markdown": "^1.0.0-beta.2",
    "eslint-plugin-node": "^2.0.0",
    "postcss-less": "^0.14.0",
    "postcss-nested": "^1.0.0",
    "postcss-scss": "^0.1.8",
    "remark-cli": "^1.0.0",
    "remark-lint": "^4.0.2",
    "rimraf": "^2.5.3"
  },
  "engines": {
    "node": ">=4"
  }
}
