    "postcss-selector-parser": "^2.0.0"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.5.0",
    "eslint": "^2.10.2",
    "eslint-config-google": "^0.5.0",
